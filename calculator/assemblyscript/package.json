{
  "name": "calculator",
  "version": "0.1.0",
  "description": "AssemblyScript version of the calculator actor example",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "asc index.ts -b calculator.wasm --use abort=index/abort --optimize",
    "asbuild:untouched": "asc index.ts -b untouched.wasm -t untouched.wat --validate --sourceMap --debug",
    "asbuild:optimized": "asc index.ts -b optimized.wasm -t optimized.wat --validate --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "author": "Matt Gilbride",
  "license": "Apache-2.0",
  "dependencies": {
    "@wapc/as-guest": "^v0.2.1",
    "@wapc/as-msgpack": "^0.1.11",
    "@wasmcloud/actor-core": "^0.1.1",
    "@wasmcloud/actor-http-server": "^0.1.2",
    "assemblyscript-json": "git+https://github.com/nearprotocol/assemblyscript-json"
  },
  "devDependencies": {
    "assemblyscript": "^0.17.1",
    "prettier": "^2.0.2"
  }
}
